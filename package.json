  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path=dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng build --watch --output-path=dist",
    "electron": "electron .",
    "pack": "electron-packager . fund --out=bin --asar --ignore=\"(node_modules|src|e2e|\\.conf\\.js|tsconfig\\.json|tslint\\.json|\\.angular-cli\\.json|\\.editorconfig|\\.git)\"",
    "pack:win32-x64": "npm run pack -- --platform=win32 --arch=x64",
    "postpack:win32-x64": "\"node_modules\\rcedit\\bin\\rcedit.exe\" ./bin/fund-win32-x64/fund.exe --set-icon ./dist/favicon.ico",
    "publish": "npm run build && npm run pack",
    "publish:win32-x64": "npm run build && npm run pack:win32-x64"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
